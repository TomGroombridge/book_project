

<%= form_for @book do |f| %>
	<p>
	<%= f.label :title %> <br>
	<%= f.text_field :title %>
	</p>
	<p>
	<%= f.label :author %> <br>
	<%= f.text_field :author %>
	</p>
	<p>
	<%= f.label :image %> <br>
	<%= f.text_field :image %>
	</p>
	<p>
	<%= f.label :url %> <br>
	<%= f.text_field :url %>
	</p>
	<p>
	<%= f.label :category %> <br>
	<%= f.text_field :category %>
	</p>
	<%= f.submit "Submit" %> 

<% end %>