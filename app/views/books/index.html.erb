
<% @books.each do |book| %>

<p>
	<strong>Title:</strong>
	<%= book.title %>
	<br>
	<strong>Author:</strong>
	<%= book.author %>
	<br>
	<strong>Image:</strong>
	<%= book.image_url %>
	<br>
	<strong>Url:</strong>
	<%= book.detail_page_url %>
	<br>
	<strong>Salesrank:</strong>
	<%= book.sales_rank %>
	<br>
</p>

<% end %>